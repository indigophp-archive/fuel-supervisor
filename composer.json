{
    "name": "indigophp/fuel-supervisor",
    "type": "fuel-package",
    "description": "Fuel wrapper around Indigo Supervisor package",
    "keywords": ["supervisor", "process manager"],
    "homepage": "https://indigophp.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "composer/installers": "~1.0",
        "indigophp/fuel-core": "~1.0.0",
        "indigophp/supervisor": "*"
    },
    "require-dev": {
        "indigophp/supervisor": "dev-develop",
        "codeception/codeception": "~1.8.0",
        "codeception/mockery-module": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Fuel\\": "classes/"
        }
    },
    "extra": {
        "installer-name": "supervisor"
    },
    "minimum-stability": "dev"
}
